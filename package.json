{
   "name": "i18nextension",
   "displayName": "Automatic i18n-Extension",
   "version": "3.0.0",
   "description": "Automatic i18n Extension",
   "publisher": "DamienArriens",
   "engines": {
      "vscode": "^1.69.1"
   },
   "categories": [
      "Formatters",
      "Programming Languages",
      "Snippets"
   ],
   "activationEvents": [
      "onStartupFinished"
   ],
   "main": "./out/extension.js",
   "contributes": {
      "commands": [
         {
            "command": "extension.i18nExtension",
            "title": "i18nExtension"
         },
         {
            "command": "extension.activated",
            "title": "activated"
         },
         {
            "command": "extension.onSave",
            "title": "onSave"
         }
      ]
   },
   "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "lint": "eslint . --ext .ts,.tsx",
      "watch": "tsc -watch -p ./"
   },
   "author": {
      "name": "Damien Arriens",
      "email": "damien.arriens@swan.de"
   },
   "repository": {
      "type": "github",
      "url": "https://github.com/DamienArriens/i18nExtension"
   },
   "license": "ISC",
   "devDependencies": {
      "@types/node": "^18.0.3",
      "@types/vscode": "^1.68.1",
      "@typescript-eslint/eslint-plugin": "^5.30.5",
      "@typescript-eslint/parser": "^5.30.5",
      "deepl-node": "^1.3.1",
      "eslint": "^8.19.0",
      "typescript": "^4.7.4"
   }
}
